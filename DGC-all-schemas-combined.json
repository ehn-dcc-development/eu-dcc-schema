    "test-result": {
      "description": "EU eHealthNetwork: Value Sets for Digital Green Certificates. version 1.0, 2021-04-16, section 2.9",
      "type": "string",
      "enum": [
        "260415000",
        "260373001"
      ]
    }
  }
}
